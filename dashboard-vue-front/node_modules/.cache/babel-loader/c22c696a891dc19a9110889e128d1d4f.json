orderRadius: 4,\r\n            borderSkipped: false,\r\n            backgroundColor: \"#4CAF50\",\r\n            borderColor: \"rgba(75, 192, 192, 1)\",\r\n            data: [...this.chart.datasets.data],\r\n            maxBarThickness: 12, // Ajusté\r\n          },\r\n        ],\r\n      },\r\n      options: {\r\n        responsive: true,\r\n        maintainAspectRatio: false,\r\n        plugins: {\r\n          legend: {\r\n            display: false,\r\n          },\r\n        },\r\n        interaction: {\r\n          intersect: false,\r\n          mode: \"index\",\r\n        },\r\n        scales: {\r\n          y: {\r\n            ticks: {\r\n              color: \"#fff\", // Écriture blanche pour l'axe Y\r\n              font: {\r\n                size: 14, // Taille de la police\r\n              },\r\n            },\r\n            grid: {\r\n              color: \"rgba(255, 255, 255, 0.1)\", // Couleur blanche transparente pour la grille\r\n            },\r\n          },\r\n          x: {\r\n            ticks: {\r\n              color: \"#fff\", // Écriture blanche pour l'axe X\r\n              font: {\r\n                size: 14,\r\n              },\r\n            },\r\n            grid: {\r\n              display: false, // Pas de grille pour l'axe X\r\n            },\r\n          },\r\n        },\r\n\r\n      },\r\n    });\r\n    // const canvas = document.getElementById(this.id);\r\n    // canvas.style.height = \"400px\";\r\n  }\r\n\r\n};\r\n</script>\r\n"],"mappings":"AAoDA,OAAOA,KAAI,MAAO,eAAe;AACjC,SAASC,eAAc,QAAS,8BAA8B;AAE9D,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAK,EAAE;IACLC,EAAE,EAAE;MACFC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,KAAK,EAAE;MACLH,IAAI,EAAEC,MAAM;MACZG,OAAO,EAAE;IACX,CAAC;IACDC,KAAK,EAAE;MACLL,IAAI,EAAEC,MAAM;MACZG,OAAO,EAAE;IACX,CAAC;IACDE,WAAW,EAAE;MACXN,IAAI,EAAEC,MAAM;MACZG,OAAO,EAAE;IACX,CAAC;IACDG,KAAK,EAAE;MACLP,IAAI,EAAEQ,MAAM;MACZN,QAAQ,EAAE,IAAI;MACdO,MAAM,EAAEC,KAAK;MACbC,QAAQ,EAAE;QACRX