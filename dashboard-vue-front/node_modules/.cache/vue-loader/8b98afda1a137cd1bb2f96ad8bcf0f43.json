: {\r\n          y: {\r\n            grid: {\r\n              drawBorder: false,\r\n              display: false,\r\n              drawOnChartArea: false,\r\n              drawTicks: false,\r\n            },\r\n            ticks: {\r\n              suggestedMin: 0,\r\n              suggestedMax: 500,\r\n              beginAtZero: true,\r\n              padding: 15,\r\n              font: {\r\n                size: 14,\r\n                family: \"Open Sans\",\r\n                style: \"normal\",\r\n                lineHeight: 2,\r\n              },\r\n              color: \"#fff\",\r\n            },\r\n          },\r\n          x: {\r\n            grid: {\r\n              drawBorder: false,\r\n              display: false,\r\n              drawOnChartArea: false,\r\n              drawTicks: false,\r\n            },\r\n            ticks: {\r\n              display: false,\r\n            },\r\n          },\r\n        },\r\n      },\r\n    });\r\n  },\r\n};\r\n</script>\r\n"]}]}